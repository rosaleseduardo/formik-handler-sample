{
  "name": "formik-handler-sample",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test:dev": "vitest --ui",
    "test:prod": "vitest --run",
    "coverage": "vitest run --coverage",
