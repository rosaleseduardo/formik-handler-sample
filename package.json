    "commit:changes": "git add -A && cz",
    "ci": "pnpm install --frozen-lockfile"
  },
  "dependencies": {
    "@emotion/react": "11.13.3",
    "@emotion/styled": "11.13.0",
    "@hookform/resolvers": "3.9.0",
